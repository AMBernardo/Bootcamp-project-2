<div id="home">
  <div class="container">
    <div class="row">
      <div class="col s12 m12 l12 xl12">
        <h1 class="animated fadeInDown slower greeting">{{msg}}</h1>
        <h2 class="animated zoomIn slower question">What's My Worth?</h2>
        <form class="animated fadeInUp slower" action="POST">
          <div class="row">
            <div class="input-field col s12 m12 l12 xl12">
              <i class="material-icons prefix">edit</i>
              <input type="text" placeholder="Enter Search Item">
              <label for="search">Search:</label>
            </div>
          </div>
          <div class="row">
            <div class="col offset-s5">
              <button class="btn waves-effect waves-light submit" >Worth It?
                <i class="material-icons right">send</i>
              </button>
            </div>
          </div>
        </form>
        <br>
        <div class="divider"></div>
      </div>
    </div>
  </div>
